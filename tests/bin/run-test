#!/bin/bash
x=test1

echo "./tests/$x" | ../bin/shibuya 2>testlog
MD1=`md5sum testlog | awk '{print $1}'`
MD2=`md5sum ./results/$x | awk '{print $1}'`

echo -n " Test: $x    "

if [ "x$MD1" != "x$MD2" ]; then
	echo "p-fail"
	diff -Nuarp testlog ./results/$x > ./diffpool/$x.fail.diff
else
	echo "OK!"
fi
